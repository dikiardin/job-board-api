    "@types/pdfkit": "^0.17.3",
    "@types/qrcode": "^1.5.5",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.4",
    "google-auth-library": "^10.4.1",
    "helmet": "^7.1.0",
    "joi": "^18.0.1",
